{
    "extends": [
        ":enableRenovate",
        ":npm",
        ":unpublishSafe",
        ":semanticCommitType(deps)",
        ":semanticCommitScope(renovate)",
        ":prNotPending",
        ":prHourlyLimitNone",
        ":label(dependencies)",
        ":automergeMinor",
        ":pinDependencies",
        ":pinDevDependencies"
    ],
    "ignorePaths": [
        "**/node_modules/**"
    ],
    "suppressNotifications": [
        "prIgnoreNotification"
    ]
}
